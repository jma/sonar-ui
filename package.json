{
  "name": "@rero/sonar-ui",
  "version": "14.0.1",
  "description": "User interface for SONAR.",
  "keywords": [
    "rero",
    "invenio",
    "angular"
  ],
  "homepage": "https://github.com/rero/sonar-ui#readme",
  "repository": {
    "type": "git",
